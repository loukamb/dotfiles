set nocompatible
set number
set tabstop=4
set shiftwidth=4
set expandtab
set smarttab
set ai
set si
set wrap
set ruler
set showmatch
set cursorline
set nobackup
set nowb
set noswapfile
set mouse=a

filetype on
filetype plugin on
filetype indent on

syntax enable
set regexpengine=0
try
    colorscheme desert
catch
endtry
set background=dark

